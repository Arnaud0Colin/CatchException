@model IEnumerable<CatchException.mvc.Models.Journal>

@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
   
</div>

<div class="row">
    <div class="col-md-4">
        @if (Model == null || Model.Count() <= 0)
        {
            <p>No Error found</p>
        }
        else
        {
            foreach (var Error in Model)
            {
                @Error.GetException();
            }
        }
    </div>
</div>
